/* pages/mine/mine.wxss */
page{background: #f6f6f6;padding-bottom: 100rpx;}
.user-base-info{
    position: relative;
    background: #1a1a1a;
    color: #fff;
    padding: 50rpx 0;
    text-align: center;
    background-image: url("data:image/png;base64,");
    background-repeat: no-repeat;
    background-size: 100% 100%;
}
.avatar-image{
    border-radius: 100%;
    width: 150rpx;
    height: 150rpx;
    display: block;
    margin: 0 auto;
    /*border: 3px solid #f1f1f1;*/
}
.user-info_text{
    margin-top: 26rpx;font-weight: normal;
    font-size: 34rpx;
}

.user-info_phone{
    margin-top: 25rpx;font-weight: normal;
    font-size: 28rpx;
}
.group-wrapper{
    margin-top: 20rpx;
}
.group-item-wrapper{background: #fff;
   
}

.group-item-wrapper-hover{background: #eee;
   
}
.group-item{
    display: flex;
    flex-direction: row;
    justify-content: flex-start;
    align-items: center;
    position: relative;
    margin: 0 40rpx;
    border-bottom: 1rpx solid #DDDDDD;
    padding: 43rpx 0;
    color: #000;
}
.no-border{border: none;}
.group-item-title{
    padding-left: 0rpx;
    font-size: 30rpx;
    color: RGBA(51, 51, 51, 1);
}
.group-item-arrow{
    background-image: url('data:image/png;base64,');
    background-size: contain;
    background-repeat: no-repeat;
    width: 14rpx;
    height: 25rpx;
    position: absolute;
    right: 0rpx;
    top: 50%;
    transform:translateY(-50%);
}
.logout-text{
    text-align: center;
    font-size: 15px;
    padding: 10rpx 0;
    color: #325782;
    margin-top: 60rpx;
}
.logout-text text{
    border-bottom: 1px solid #325782;
}
.user-tag{
    border: 1rpx solid RGBA(255, 255, 255, 1);
    border-radius: 8rpx;
    min-width:90rpx;
    background: RGBA(46, 46, 46, 0.8);
    height:40rpx;line-height: 40rpx;
    padding: 0 8px;
    text-align:center;
    font-size:26rpx;
    position:absolute;
    top:100rpx;
    right:100rpx;
}